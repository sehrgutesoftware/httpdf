{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "httpdf Example",
    "type": "object",
    "properties": {
        "title": {
            "type": "string"
        },
        "date": {
            "type": "string",
            "format": "date-time"
        },
        "list": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "amount": {
                        "type": "number",
                        "minimum": 0,
                        "maximum": 1000,
                        "multipleOf": 0.1
                    }
                }
            }
        }
    }
}
